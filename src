import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        int senha;

        Scanner sc = new Scanner(System.in);

        System.out.print("SENHA: ");
        senha = sc.nextInt();

        while (senha != 2002) {
            System.out.print("Senha inv√°lida!");
            senha = sc.nextInt();
        }

        System.out.print("Acesso permitido!");

        sc.close();
    }
}
